$("<%=j @rows_to_remove %>").remove();
$("<%=j @rows_to_uncheck %>").prop("checked",false);
<% if params[:move] %>
  <% if @failed_barcodes.length > 0 %>
    Materialize.toast("Error moving: <%= @failed_barcodes %>", 5000, "red");
  <% else %>
    Materialize.toast("Completed moving book loans", 5000, "green");
  <% end %>
<% elsif params[:batch] %>
  window.location = "<%= @return_path %>"
  Materialize.toast("Book loans records updated.", 5000, "green");
<% elsif params[:delete] %>
  Materialize.toast("Book loans deleted", 5000, "green");
<% elsif params[:dispose] %>
  <% if @dispose.present? %>
    Materialize.toast("Book loans disposed", 5000, "green");
  <% else %>
    Materialize.toast("Error disposed: <%= @failed_disposes %>", 5000, "red");
  <% end %>
<% end %>
update_borrowers_menu();
toggle_menu();
